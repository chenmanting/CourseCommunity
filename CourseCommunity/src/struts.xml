<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="default" namespace="/" extends="struts-default"/>
	
	<package name="users" namespace="/users" extends="default">
		<action name="*_*" class="com.cmt.action.{1}Action" method="{2}">
			<result name="success">/main.jsp</result>
			<result name="login_success">/main.jsp</result>
			<result name="login_failure">/login.jsp</result>
			<result name="registe_success">/main.jsp</result>
			<result name="registe_failure">/register.jsp</result>
		</action>
	</package>
	
	
	
</struts>    
